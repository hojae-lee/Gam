<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC 
    "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN" 
    "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>
    <definition name="default-layout" template="/WEB-INF/tiles/html/layouts/default-layout.jsp">
        <put-attribute name="head" value="/WEB-INF/tiles/html/layouts/header.jsp" />
        <put-attribute name="footer" value="/WEB-INF/tiles/html/layouts/footer.jsp" />
    </definition>
    
    <definition name="main/test" extends="default-layout">
        <put-attribute name="content" value="/WEB-INF/views/main/test.jsp" />
    </definition>
    
    <definition name="ms-layout" template="/WEB-INF/tiles/html/layouts/ms-layout.jsp">
    	<put-attribute name="ms-header" value="/WEB-INF/tiles/html/layouts/ms-header.jsp" />
        <put-attribute name="ms-footer" value="/WEB-INF/tiles/html/layouts/ms-footer.jsp" />
    </definition>
    
    <definition name="main/mstest" extends="ms-layout">
        <put-attribute name="ms-content" value="/WEB-INF/views/main/mstest.jsp" />
    </definition>
</tiles-definitions>